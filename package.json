{
  "name": "gatsby-source-strava-activities",
  "version": "1.0.1",
  "description": "Gatsby plugin that fetches athlete activities from Strava",
  "scripts": {
    "test": "yarn test:eslint && yarn test:prettier",
    "test:eslint": "eslint . --ignore-path .gitignore --ext .jsx --ext .js",
    "test:prettier": "prettier \"**/*.{js,jsx,css,scss,json,md,mdx,html}\" --list-different",
    "prepublishOnly": "microbundle --entry src/gatsby-node.js --output gatsby-node.js --target node --format cjs",
    "format": "prettier \"**/*.{js,jsx,css,scss,json,md,mdx,html}\" --write",
    "release": "git checkout master && git pull && yarn test && yarn publish && git push --follow-tags"
  },
  "repository": "https://github.com/danoc/gatsby-source-strava-activities",
  "author": "Daniel Oâ€™Connor <daniel@danoc.me>",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.2.0",
    "query-string": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.0.0-rc.15",
    "microbundle": "^0.11.0",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "keywords": [
    "gatsby",
    "strava",
    "gatsby-plugin"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
