    "prepublishOnly": "babel src --out-dir .",
    "release": "git checkout master && git pull && yarn test && yarn publish && git push --follow-tags"
  },
  "main": "gatsby-node.js",
  "repository": "https://github.com/danoc/gatsby-source-strava-activities",
  "author": "Daniel Oâ€™Connor <daniel@danoc.me>",
  "license": "MIT",
  "dependencies": {
    "crypto": "^1.0.1",
    "node-fetch": "^2.2.0",
    "query-string": "^6.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.0.0-rc.15",
    "lint-staged": "^7.3.0",
